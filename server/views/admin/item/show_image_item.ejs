<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
  tabindex="0"
>
  <!-- DataTales Example -->
  <div class="row">
    <% for(let i=0; i < item.imageId.length; i++) {%>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img 
            src="<%= process.env.IMAGE_HOST %>/<%= item.imageId[i].imageUrl.slice(0, 6) %>%2F<%= item.imageId[i].imageUrl.slice(7) %>?alt=media&token=<%= process.env.ACCESS_TOKEN %>"
            alt="image"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <%} %>
  </div>
  <a href="/admin/item" class="btn btn-warning">Back</a>
</div>
